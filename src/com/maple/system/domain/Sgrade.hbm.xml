<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.maple.system.domain">
	
	<class name="Sgrade" table="sgrade">
		<composite-id>
			<key-property name="idstudent" column="idstudent"/>
			<key-property name="idcourse" column="idcourse"/>
		</composite-id>
		<property name="grade" />
		
	</class>
	
</hibernate-mapping>